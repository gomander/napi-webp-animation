{
  "name": "napi-webp-animation",
  "version": "0.2.1",
  "description": "A fully typed WebP animation package for NodeJS using Rust",
  "author": "Gunnar Omander <gunnar@gomander.dev> (https://gomander.dev)",
  "license": "MIT",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "format": "run-p format:toml format:rs",
    "format:prettier": "prettier . -w",
    "format:toml": "taplo format",
    "format:rs": "cargo fmt",
    "lint": "oxlint",
    "create-npm-dirs": "napi create-npm-dirs",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test/main.test.mjs",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/canvas": "~0.1.83",
    "@napi-rs/cli": "^3.5.0",
    "@napi-rs/wasm-runtime": "^1.1.0",
    "@taplo/cli": "^0.7.0",
    "@types/node": "^24.10.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.31.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "napi": {
    "binaryName": "napi-webp-animation",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc"
    ]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "default": "./index.js"
    }
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "engines": {
    "node": ">= 18"
  },
  "repository": {
    "url": "git+ssh://git@github.com/gomander/napi-webp-animation.git",
    "type": "git"
  },
  "homepage": "https://github.com/gomander/napi-webp-animation#readme",
  "bugs": "https://github.com/gomander/napi-webp-animation/issues",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api",
    "webp",
    "webp-animation"
  ],
  "packageManager": "pnpm@10.20.0"
}
